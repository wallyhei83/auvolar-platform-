{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "outputDirectory": "apps/portal/.next",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "functions": {
    "apps/portal/src/app/api/**": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  }
}